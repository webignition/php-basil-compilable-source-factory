{
    "name": "webignition/php-basil-compilable-source-factory",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/php-basil-compilable-source-factory",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\BasilCompilableSourceFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\BasilCompilableSourceFactory\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR12",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always --group=poc208",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "webignition/php-basil-compilation-source": ">=0.64,<1",
        "webignition/php-basil-models": ">=0.91,<1",
        "webignition/symfony-panther-dom-crawler-navigator": ">=0.13,<1",
        "webignition/webdriver-element-inspector": ">=0.6,<1",
        "webignition/webdriver-element-mutator": ">=0.2,<1",
        "webignition/basil-code-generator": ">=0.6,<1",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan-mockery": "^0.11.3",
        "phpstan/extension-installer": "^1.0",
        "webignition/php-dom-element-locator": ">=0.1,<1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^0.11.5",
        "webignition/base-panther-test-case": "^0.2.0",
        "webignition/basil-action-generator": ">=0.2,<1",
        "webignition/basil-assertion-generator": ">=0.2,<1",
        "webignition/basil-parser": ">=0.5,<1"
    }
}
